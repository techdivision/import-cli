{
    "name": "techdivision/import-cli",
    "description": "Commands and Helpers to implement a CLI providing single-threaded Magento 2 importing functionality based on Pacemaker",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ramsey/uuid": "^4.7.6",
        "symfony/config": "v4.4.44",
        "symfony/console": "v4.4.49",
        "symfony/expression-language": "v6.4.8",
        "symfony/dependency-injection": "v4.4.49",
        "techdivision/import-app-simple": "*",
        "techdivision/import-configuration-jms": "*"
    },
    "require-dev": {
        "doctrine/dbal": "^4.0.4",
        "pdepend/pdepend": "^2.16.2",
        "phpmd/phpmd": "^2.15.0",
        "phpunit/phpunit": "^11.2.5",
        "sebastian/phpcpd": "^2.0.1",
        "squizlabs/php_codesniffer": "^3.10.1",
        "consolidation/robo": "^4.0.2",
        "mikey179/vfsstream": "v1.6.11",
        "symfony/http-kernel": "v4.4.51"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TechDivision\\Import\\Cli\\": [
                "src/",
                "symfony/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechDivision\\Import\\Cli\\": [
                "tests/unit"
            ]
        }
    }
}
